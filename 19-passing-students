public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    if(passedAssignments && passedExam ){
        return True;
    }
    else if(passedAssignments  && passedProject ){
        return true;
    }
    else if(passedExam && passedProject ){
        return true
    }
    else{
        return false;
    }
}