public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    list<Boolean> BL = new list<Boolean>();
    int count = 0;
    BL.add(passedAssignments);
    BL.add(passedExam);
    BL.add(passedProject);

    for(Boolean B:BL){
        if(B == True){
            Count++;
        }  
    }
    if(count>=2){
        return True;
    }
    else{
        return False;
    }
}