public String aOrAn(String word) {
    //code here
    List <String> vowels = new List<String>{'a','e','i','o','u'};
    for(string vowel:vowels)
    {
        if (word.startsWith(vowel))
        {
            System.assertEquals('an '+word);
        }
        else
        {
        System.assertEquals('a '+word);
        }
    }
}